{
  "name": "south-bay-sit-n-stay-june-2019",
  "version": "1.0.0",
  "description": "A site to book pet care services built with React + Redux , node.js , express , and mongoDB",
  "main": "index.js",
  "engines": {
    "node": "10.15.0"
  },
  "scripts": {
    "server": "nodemon server.js",
    "client": "webpack-dev-server --mode development --open",
    "start": "node server.js",
    "dev": "concurrently --kill-others \"npm run client\" \"npm run server\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install && webpack --mode production"
  },
